<h1>Post List</h1>

<table>
  <thead>
  <td>title</td>
  <td>content</td>
  <td>action</td>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td>
        <%= button_to 'Delete', post_path(post), method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
